<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.gaion.scas.analysis.mapper.TreeInfoMapper">
	<select id="findTree" resultType="kr.co.gaion.scas.analysis.model.TreeInfo">
		select * 
		from TREEINFO
	</select>
	
	<select id="findFirstHeader" parameterType="Integer" resultType="kr.co.gaion.scas.analysis.model.TreeInfo">
		select * 
		from TREEINFO
		where trinfoid = #{trinfoid}
	</select>
	
	<select id="findHeader" parameterType="Integer" resultType="kr.co.gaion.scas.analysis.model.TreeInfo">
		select * 
		from TREEINFO
		where trinfohead = #{trinfohead}
	</select>
	
	<select id="findTreeInfoIdByCode" parameterType="String" resultType="Integer">
		select trinfoid
		from TREEINFO
		where trinfocode = #{treeinfocode}
	</select>
	
	<select id="findByCode" parameterType="String" resultType="kr.co.gaion.scas.analysis.model.TreeInfo">
		select *
		from TREEINFO
		where trinfocode = #{code}
	</select>
</mapper>